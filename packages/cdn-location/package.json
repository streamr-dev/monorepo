{
  "name": "@streamr/cdn-location",
  "version": "102.0.0-beta.0",
  "description": "Library for getting own approximate location by querying CDN servers",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streamr-dev/network.git",
    "directory": "packages/cdn-location"
  },
  "type": "module",
  "module": "src/exports.ts",
  "types": "build/src/exports.d.ts",
  "license": "Apache-2.0",
  "author": "Streamr Network AG <contact@streamr.network>",
  "scripts": {
    "build": "tsc -b",
    "zero": "rm -rf build dist",
    "postbuild": "rollup -c && dts-bundle-generator --no-banner -o dist/bundle.d.ts --project=tsconfig.dist.json src/exports.ts && tsx scripts/rewrite-package.ts",
    "clean": "jest --clearCache || true; rm -rf dist *.tsbuildinfo node_modules/.cache || true",
    "eslint": "eslint --cache --cache-location=node_modules/.cache/.eslintcache/ '*/**/*.{js,ts}'",
    "test": "jest test/integration",
    "test-integration": "jest test/integration",
    "prepare-for-tsp-solver": "./data-generation/prepareForTSPSolver.sh",
    "generate-data-from-tsp-solution": "./data-generation/generateDataFromTSPSolverResult.sh"
  },
  "dependencies": {
    "haversine": "^1.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@streamr/utils": "^102.0.0-beta.0",
    "@types/haversine": "^1.1.8",
    "prettier": "^3.3.3",
    "rollup": "^4.26.0",
    "rollup-plugin-copy": "^3.5.0",
    "tsx": "^4.19.2"
  }
}
