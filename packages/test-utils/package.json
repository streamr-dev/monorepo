{
  "name": "@streamr/test-utils",
  "version": "102.0.0-beta.0",
  "description": "A collection of shared test utilities",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streamr-dev/network.git",
    "directory": "packages/test-utils"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./customMatchers": {
      "import": "./src/customMatchers.ts",
      "types": "./src/customMatchers.ts"
    },
    "./setupCustomMatchers": {
      "import": "./src/setupCustomMatchers.ts",
      "types": "./src/setupCustomMatchers.ts"
    },
    "./customMatcherTypes": {
      "types": "./src/customMatchersTypes.ts"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "postbuild": "bash scripts/postbuild.sh",
    "clean": "jest --clearCache || true; rm -rf dist *.tsbuildinfo node_modules/.cache || true",
    "check": "tsc --noEmit -p tsconfig.esm.json",
    "test": "jest",
    "eslint": "eslint --cache --cache-location=node_modules/.cache/.eslintcache/ '*/**/*.{js,ts}'"
  },
  "author": "Streamr Network AG <contact@streamr.network>",
  "license": "Apache-2.0",
  "dependencies": {
    "@streamr/utils": "^102.0.0-beta.0",
    "cors": "^2.8.5",
    "ethers": "^6.13.0",
    "express": "^4.21.1",
    "jest": "^29.7.0",
    "jest-matcher-utils": "^29.7.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node-fetch": "^2.6.4",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2"
  }
}
