{
  "name": "streamr-network",
  "version": "25.0.0",
  "description": "Minimal and extendable implementation of the Streamr Network network node.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streamr-dev/network.git"
  },
  "bin": {
    "network": "bin/network.js",
    "publisher": "bin/publisher.js",
    "subscriber": "bin/subscriber.js",
    "tracker": "bin/tracker.js"
  },
  "main": "dist/composition.js",
  "types": "dist/composition.d.ts",
  "scripts": {
    "prepare": "tsc -p tsconfig-build.json",
    "build": "tsc -p tsconfig-build.json",
    "tracker": "node $NODE_DEBUG_OPTION bin/tracker.js",
    "pub": "node $NODE_DEBUG_OPTION bin/publisher.js",
    "pub-1": "node $NODE_DEBUG_OPTION bin/publisher.js --port=30323",
    "pub-2": "node $NODE_DEBUG_OPTION bin/publisher.js --port=30333",
    "sub": "node $NODE_DEBUG_OPTION bin/subscriber.js",
    "sub-1": "node $NODE_DEBUG_OPTION bin/subscriber.js --port=30335",
    "sub-2": "node $NODE_DEBUG_OPTION bin/subscriber.js --port=30345",
    "test": "jest",
    "test-unit": "jest test/unit",
    "test-browser": "jest --config browser-jest.config.js",
    "coverage": "jest --coverage",
    "test-integration": "jest test/integration",
    "test-flakey": "jest test/flakey",
    "test-types": "tsc --noEmit --project ./tsconfig.json ",
    "eslint": "eslint .",
    "network": "node $NODE_DEBUG_OPTION bin/network.js",
    "docs": "typedoc --options typedoc.js",
    "serve": "node test/server.js"
  },
  "author": "Streamr Network AG <contact@streamr.network>",
  "license": "STREAMR NETWORK OPEN SOURCE LICENSE",
  "dependencies": {
    "cancelable-promise": "^3.2.3",
    "commander": "^7.2.0",
    "geoip-lite": "^1.4.2",
    "heap": "^0.2.6",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "node-datachannel": "^0.1.4",
    "pino": "^6.11.3",
    "speedometer": "^1.1.0",
    "streamr-client-protocol": "^8.2.1",
    "strict-event-emitter-types": "^2.0.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v18.14.0",
    "uuid": "^8.3.2",
    "ws": "^7.4.5"
  },
  "devDependencies": {
    "@streamr/dev-config": "^1.0.0",
    "@types/expect-puppeteer": "^4.4.5",
    "@types/express": "^4.17.12",
    "@types/geoip-lite": "^1.4.1",
    "@types/heap": "^0.2.28",
    "@types/jest": "^26.0.23",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/lodash": "^4.14.169",
    "@types/lru-cache": "^5.1.0",
    "@types/pino": "^6.3.8",
    "@types/puppeteer": "^5.4.3",
    "@types/uuid": "^8.3.0",
    "@types/webpack-dev-middleware": "^5.0.0",
    "@types/ws": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-streamr-ts": "^3.0.1",
    "eslint-plugin-promise": "^5.1.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-cli": "^27.0.5",
    "jest-puppeteer": "^5.0.4",
    "path-browserify": "^1.0.1",
    "puppeteer": "^10.0.0",
    "streamr-test-utils": "^1.3.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.3",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4",
    "webpack": "^5.40.0",
    "webpack-dev-middleware": "^5.0.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3",
    "pino-pretty": "^4.7.1",
    "utf-8-validate": "^5.0.5"
  }
}
